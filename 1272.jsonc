{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": { 
    "source": "windows", 
    "padding": { "top": 4 } 
  },
  "display": { 
    "separator": " 󰑃  " 
  },
  "modules": [
    // DISTRO SECTION
    { "type": "custom", "format": "╔═════════════🖥️ DISTRO════════════" },
    { "type": "os", "key": "│ 🖥️ OS", "keyColor": "yellow" },
    { "type": "kernel", "key": "│ ⚙️ Kernel", "keyColor": "yellow" },
    { "type": "packages", "key": "│ 📦 Packages", "keyColor": "yellow" },
    {
      "type": "command",
      "key": "│ ⏳ Install Age",
      "keyColor": "yellow",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
    },
    { "type": "shell", "key": "│ 🐚 Shell", "keyColor": "yellow" },
    {
      "type": "command",
      "key": "│ 📦 Apps",
      "keyColor": "yellow",
      "text": "C:\\Users\\numan\\Desktop\\cp.bat"
    },
    { "type": "custom", "format": "🌧 ☂ 🌞 🌂  🌧 ☂ 🌞 🌂  🌧 ☂ 🌞 🌂" },

    // DE/WM SECTION
    { "type": "custom", "format": "╔══════════════🪟 DE/WM════════════" },
    { "type": "wm", "key": "│ 🪟 DE/WM", "keyColor": "blue" },
    { "type": "wmtheme", "key": "│ 🎨 WM Theme", "keyColor": "blue" },
    { "type": "icons", "key": "│ 🎭 Icons", "keyColor": "blue" },
    { "type": "cursor", "key": "│ 🖱️ Cursor", "keyColor": "blue" },
    { "type": "terminalfont", "key": "│ 🖋️ Terminal Font", "keyColor": "blue" },
    {
      "type": "terminal",
      "key": "│ 💻 Terminal",
      "keyColor": "blue",
      "format": "{pretty-name} {version} {#37}█{#97}█ {#36}█{#96}█ {#35}█{#95}█ {#34}█{#94}█ {#33}█{#93}█ {#32}█{#92}█ {#31}█{#91}█ {#30}█{#90}█"
    },
    { "type": "custom", "format": "🌧 ☂ 🌞 🌂  🌧 ☂ 🌞 🌂  🌧 ☂ 🌞 🌂" },

    // SYSTEM SECTION
    { "type": "custom", "format": "╔═════════════🏠 SYSTEM════════════" },
    { "type": "host", "key": "│ 🏠 Host", "keyColor": "green" },
    { "type": "cpu", "key": "│ 🧠 CPU", "keyColor": "green" },
    {
      "type": "gpu",
      "key": "│ 🎮 GPU",
      "format": "{name} {temperature}",
      "keyColor": "green",
      "temp": true,
      "driverSpecific": true
    },
    { "type": "display", "key": "│ 🖥️ Display", "keyColor": "green", "compactType": "original-with-refresh-rate" },
    { "type": "memory", "key": "│ 💾 Memory", "keyColor": "green" },
    { "type": "swap", "key": "│ 🔄 Swap", "keyColor": "green" },
    { "type": "uptime", "key": "│ ⏳ Uptime", "keyColor": "green" },
    {
      "type": "users",
      "myselfOnly": true,
      "keyIcon": "👤",
      "key": "│ 👤 Login Time",
      "format": "{login-time}",
      "keyColor": "green"
    },
    { "type": "custom", "format": "🌧 ☂ 🌞 🌂  🌧 ☂ 🌞 🌂  🌧 ☂ 🌞 🌂" },

    // AUDIO/MEDIA SECTION
    { "type": "custom", "format": "╔═════════════🔊 AUDIO═════════════" },
    { "type": "sound", "key": "│ 🔊 Audio", "format": "{2}", "keyColor": "magenta" },
    { "type": "player", "key": "│ 🎶 Player", "keyColor": "magenta" },
    { "type": "media", "key": "│ 🎬 Media", "keyColor": "magenta" },
    { "type": "custom", "format": "                            🌧 ☂ 🌞 🌂                " },

    {
      "type": "custom",
      "format": "\u001b[90m  \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m  \u001b[38m  \u001b[39m  \u001b[39m    \u001b[38m  \u001b[37m  \u001b[36m  \u001b[35m  \u001b[34m  \u001b[33m  \u001b[32m  \u001b[31m  \u001b[90m "
    },
    { "type": "break" }
  ]
}
